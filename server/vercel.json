{
    "name": "ProfileBuilder",
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "index.js"
      },
      {
        "src": "//profile/:userId",
        "dest": "index.js"
      },
      {
        "src": "/",
        "dest": "build/index.html"
      },
      {
        "src": "/auth/register",
        "dest": "build/index.html"
      },
      {
        "src": "/auth",
        "dest": "index.js"
      },
      {
        "src": "/users",
        "dest": "index.js"
      },
      {
        "src": "/posts",
        "dest": "index.js"
      },
      {
        "src": "/(.+)",
        "dest": "build/$1"
      }
    ]
  }
  